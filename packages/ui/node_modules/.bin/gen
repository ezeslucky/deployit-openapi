#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/aurob/ALL CODING/Project/Project-29/deployit-api/node_modules/.pnpm/@turbo+gen@2.5.0_@types+node@22.14.0_typescript@5.8.2/node_modules/@turbo/gen/dist/node_modules:/mnt/c/Users/aurob/ALL CODING/Project/Project-29/deployit-api/node_modules/.pnpm/@turbo+gen@2.5.0_@types+node@22.14.0_typescript@5.8.2/node_modules/@turbo/gen/node_modules:/mnt/c/Users/aurob/ALL CODING/Project/Project-29/deployit-api/node_modules/.pnpm/@turbo+gen@2.5.0_@types+node@22.14.0_typescript@5.8.2/node_modules/@turbo/node_modules:/mnt/c/Users/aurob/ALL CODING/Project/Project-29/deployit-api/node_modules/.pnpm/@turbo+gen@2.5.0_@types+node@22.14.0_typescript@5.8.2/node_modules:/mnt/c/Users/aurob/ALL CODING/Project/Project-29/deployit-api/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/aurob/ALL CODING/Project/Project-29/deployit-api/node_modules/.pnpm/@turbo+gen@2.5.0_@types+node@22.14.0_typescript@5.8.2/node_modules/@turbo/gen/dist/node_modules:/mnt/c/Users/aurob/ALL CODING/Project/Project-29/deployit-api/node_modules/.pnpm/@turbo+gen@2.5.0_@types+node@22.14.0_typescript@5.8.2/node_modules/@turbo/gen/node_modules:/mnt/c/Users/aurob/ALL CODING/Project/Project-29/deployit-api/node_modules/.pnpm/@turbo+gen@2.5.0_@types+node@22.14.0_typescript@5.8.2/node_modules/@turbo/node_modules:/mnt/c/Users/aurob/ALL CODING/Project/Project-29/deployit-api/node_modules/.pnpm/@turbo+gen@2.5.0_@types+node@22.14.0_typescript@5.8.2/node_modules:/mnt/c/Users/aurob/ALL CODING/Project/Project-29/deployit-api/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@turbo/gen/dist/cli.js" "$@"
else
  exec node  "$basedir/../@turbo/gen/dist/cli.js" "$@"
fi
